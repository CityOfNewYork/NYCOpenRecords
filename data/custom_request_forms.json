    {
      "agency_ein": "0841",
      "form_name": "DOT FOIL Request",
      "form_description": "",
      "field_definitions": [
        {
          "Description of Records Requested": {
            "type": "textarea",
            "name": "dot-form1-field1",
            "required": true,
            "error_message": "<span class=\"glyphicon glyphicon-exclamation-sign\"></span>&nbsp;<strong>Error, Description of Records Requested is required.</strong> Please enter a description.",
            "max_length": "5000",
            "character_counter": true
          }
        },
        {
          "Date of Incident": {
            "type": "date",
            "name": "dot-form1-field2",
            "required": false,
            "placeholder": "MM/DD/YYYY",
            "help_text": "e.g. 03/15/2018",
            "future_date_invalid": true
          }
        },
        {
          "Location": {
            "type": "input",
            "name": "dot-form1-field3",
            "required": false,
            "help_text": "Street address; street with cross street(s) (e.g. 350 5th Avenue; or 5th Avenue and 1st Avenue; or 5th Avenue between 6th Avenue and 7th Avenue)"
          }
        },
        {
          "Borough": {
            "type": "select_dropdown",
            "name": "dot-form-1-field4",
            "values": [
              "Brooklyn",
              "Bronx",
              "Manhattan",
              "Queens",
              "Staten Island"
            ],
            "required": false
          }
        },
        {
          "Client Name": {
            "type": "input",
            "name": "dot-form1-field5",
            "placeholder": "John Smith",
            "required": false
          }
        },
        {
          "Client File Number": {
            "type": "input",
            "name": "dot-form1-field6",
            "required": false
          }
        }
      ],
      "repeatable": "1",
      "category": "1",
      "minimum_required": "0"
    },
    {
