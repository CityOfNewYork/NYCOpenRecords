openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana start
openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana force-start
openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana stop
openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana force-start
openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana force-stop
openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana status
openrecords ALL=(ALL) NOPASSWD: /etc/init.d/kibana restart